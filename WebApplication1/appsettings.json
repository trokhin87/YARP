{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/api/Auth/{**catch-all}"
        }
      },
      "friendRoute": {
        "ClusterId": "friendCluster",
        "Match": {
          "Path": "/api/Friend/{**catch-all}"
        }
      },
      "authSwagger": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/swagger/auth/{**catch-all}"
        }
      },
      "friendSwagger": {
        "ClusterId": "friendCluster",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "authDestination": {
            "Address": "http://localhost:5082/"
          }
        }
      },
      "friendCluster": {
        "Destinations": {
          "friendDestination": {
            "Address": "http://localhost:5015/"
          }
        }
      }
    }
  }

}